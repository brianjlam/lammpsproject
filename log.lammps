#--------------------------- General variables --------------------------------#
  # Input parameters.
  variable          a    equal 2.9115 # Lattice parameter.
  variable          n    equal 3      # System size.
  variable          P    equal 0      # Simulation pressure.
  variable          t    equal 100000 # Simulation time.
  variable          dt   equal 0.001  # Timestep.
  variable          dt_t equal 100    # Thermo output stride.
  variable          T    index 0 100 200 300 400 500 600 700 800 # Loop simulation temperature.

  # Simulation variables.
  variable          T0     equal 0.7*${T}    # Initial temperature.
  variable          T0     equal 0.7*0    
  variable          damp_T equal 100*${dt} # Thermostat damp parameter.
  variable          damp_T equal 100*0.001 
  variable          damp_P equal 100*${dt} # Barostat damp parameter.
  variable          damp_P equal 100*0.001 

#------------------------------------------------------------------------------#


#---------------------------- Atomic setup ------------------------------------#
  units             metal

  # Create atoms.
  lattice           bcc ${a}  # bcc lattice with unit cell size = a.
  lattice           bcc 2.9115  
Lattice spacing in x,y,z = 2.9115 2.9115 2.9115
  region            sim_box block 0 ${n} 0 ${n} 0 ${n} # xlo xhi ylo yhi ...
  region            sim_box block 0 3 0 ${n} 0 ${n} 
  region            sim_box block 0 3 0 3 0 ${n} 
  region            sim_box block 0 3 0 3 0 3 
  create_box        1 sim_box # make box using region 'sim_box'.
Created orthogonal box = (0 0 0) to (8.7345 8.7345 8.7345)
  1 by 1 by 1 MPI processor grid
  create_atoms      1 box     # place atoms within region on lattice.
Created 54 atoms

  # Interatomic potential information.
  pair_style        eam
  pair_coeff        * * Fe.eam
  neigh_modify      delay 0
  timestep          ${dt}
  timestep          0.001
#------------------------------------------------------------------------------#


#-------------------- Fixes, computes, and constraints ------------------------#
  # Integration and thermostat.
  fix               f1 all nph iso ${P} ${P} ${damp_P}
  fix               f1 all nph iso 0 ${P} ${damp_P}
  fix               f1 all nph iso 0 0 ${damp_P}
  fix               f1 all nph iso 0 0 0.1
  fix               f2 all langevin ${T} ${T} ${damp_T} 666
  fix               f2 all langevin 0 ${T} ${damp_T} 666
  fix               f2 all langevin 0 0 ${damp_T} 666
  fix               f2 all langevin 0 0 0.1 666
#------------------------------------------------------------------------------#


#--------------------------- Output setup -------------------------------------#
  # Setup output variables.
  variable          step equal step
  variable          E    equal etotal/atoms
  variable          ke   equal ke/atoms
  variable          pe   equal pe/atoms
  variable          lp   equal (xhi-xlo)/${n}
  variable          lp   equal (xhi-xlo)/3

  # Thermo output.
  thermo_style      custom step pe ke
  thermo            1000
#------------------------------------------------------------------------------#


#----------------------------- Run simulation ---------------------------------#
  # Initial temperature to accelerate equilibration.
  velocity          all create ${T0} 666 dist gaussian
  velocity          all create 0 666 dist gaussian

  # Print information to output file.
  fix               f5 all print ${dt_t} "${step} ${E} ${ke} ${pe} ${lp}"                          title "step E [eV/atom] ke [eV/atom] pe [eV/atom] latparam"                     screen no file output/thermo${T}.dat
  fix               f5 all print 100 "${step} ${E} ${ke} ${pe} ${lp}"                          title "step E [eV/atom] ke [eV/atom] pe [eV/atom] latparam"                     screen no file output/thermo${T}.dat
  fix               f5 all print 100 "${step} ${E} ${ke} ${pe} ${lp}"                          title "step E [eV/atom] ke [eV/atom] pe [eV/atom] latparam"                     screen no file output/thermo0.dat
  # Run simulation.
  run               ${t}
  run               100000
